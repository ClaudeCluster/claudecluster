{
  "name": "@claudecluster/cli",
  "version": "0.1.0",
  "description": "Command Line Interface for ClaudeCluster",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "claudecluster": "./dist/bin/claudecluster.js",
    "cc": "./dist/bin/claudecluster.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./commands": {
      "import": "./dist/commands/index.js",
      "types": "./dist/commands/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@claudecluster/core": "workspace:*",
    "@claudecluster/shared": "workspace:*",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "boxen": "^7.1.1",
    "table": "^6.8.1",
    "yaml": "^2.3.4",
    "axios": "^1.6.2",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/inquirer": "^9.0.7",
    "@types/ws": "^8.5.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "prettier": "^3.1.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "keywords": [
    "claudecluster",
    "cli",
    "distributed",
    "orchestration",
    "claude",
    "typescript"
  ],
  "author": "ClaudeCluster Team",
  "license": "MIT",
  "homepage": "https://github.com/claudecluster/claudecluster#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claudecluster/claudecluster.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/claudecluster/claudecluster/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}